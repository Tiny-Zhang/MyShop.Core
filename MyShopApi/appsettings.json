{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",


  //数据库连接字符串
  "DBConfig": {
    "DBType": "mysql", //mysql、sqlserver、oracle
    //"DBConnectionString": "Data Source=localhost;Database=5.14;User Id=root;Password=123456;CharSet=utf8;port=3306", //mysql连接字符串
    "DBConnectionString": "Data Source=127.0.0.1;Initial Catalog=5.14;User Id=sa;Password=123456", //sqlserver主库连接字符串
    "DBReadOnlyConString": "Data Source=127.0.0.1;Initial Catalog=5.14;User Id=sa;Password=123456", //sqlserver只读库连接字符串
    "DBTimeout": 180, //180s  单位秒
    "RedisConnectionString": "127.0.0.1:6379,password=123456,abortConnect=false" //redis连接字符串
  }


}
